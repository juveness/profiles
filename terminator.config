[global_config]
[keybindings]
  zoom_in = <Primary>equal
  zoom_out = <Primary>KP_Subtract
  zoom_out_all = <Primary>minus
  split_horiz = <Primary>underscore
  split_vert = <Primary>plus
  copy = <Primary><Shift>c
  paste = <Primary><Shift>v
[profiles]
  [[default]]
    background_darkness = 1.0
    background_type = transparent
    palette = "#000000:#cc0000:#4e9a06:#c4a000:#3465a4:#75507b:#06989a:#d3d7cf:#555753:#ef2929:#8ae234:#fce94f:#729fcf:#ad7fa8:#34e2e2:#eeeeec"
    title_hide_sizetext = True
    title_transmit_bg_color = "#4e9a06"
    title_receive_bg_color = "#c061cb"
    title_inactive_bg_color = "#3d3846"
  [[personnal-terminator]]
    background_darkness = 1.0
    background_type = transparent
    palette = "#000000:#cc0000:#4e9a06:#c4a000:#3465a4:#75507b:#06989a:#d3d7cf:#555753:#ef2929:#8ae234:#fce94f:#729fcf:#ad7fa8:#34e2e2:#eeeeec"
    title_hide_sizetext = True
    title_transmit_bg_color = "#4e9a06"
    title_receive_bg_color = "#c061cb"
    title_inactive_bg_color = "#3d3846"
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
[plugins]
